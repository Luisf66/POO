import 'package:flutter/material.dart';

class MinhaCaixa extends StatelessWidget {
  MinhaCaixa();

    @override Widget build(BuildContext){
    return BottomNavigationBar(
      items: const [BottomNavigationBarItem(
        label: "Home",
        icon: Icon(Icons.home),
      ),
      BottomNavigationBarItem(
        label: "Localização",
        icon: Icon(Icons.location_on)),
      BottomNavigationBarItem(
        label: "Android",
        icon: Icon(Icons.android))
      ]);
  }
}

void main() {
  MaterialApp app = MaterialApp(
      theme: ThemeData(
          brightness: Brightness.dark,
          primaryColor: Colors.cyan,
          fontFamily: 'Georgia'),
      home: Scaffold(
        appBar: AppBar(title: Text("Tela inicial")),
        body: Center(
          child: Column(
            children: [
              Expanded(child: Text("Texto 1")),
              Expanded(child: Text("Texto 2")),
              Expanded(child: Text("Texto 3")),
              Expanded(child: Text("Texto 4")),
              Expanded(child: Text("Texto 5")),
            ],
          ),
        ),
        bottomNavigationBar: MinhaCaixa()),
      );
  runApp(app);
}
